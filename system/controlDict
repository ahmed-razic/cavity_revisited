FoamFile
{
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          incompressibleFluid;

startFrom       startTime;

startTime       0;

stopAt          endTime;
//stopAt          writeNow;

endTime         50;

deltaT          1;

writeControl    timeStep;       //chceck later

writeInterval   1;              //check later

purgeWrite      0;              //check later

writeFormat     ascii;

writePresision  6;

timeFormat      general;

timePrecision   6;

runTimeModifiable   true;

